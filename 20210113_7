#include <stdio.h>


int main(void){

short int a = 24212;
printf("%d\n", a);

int b = -1357674;
printf("%d\n", b);

int c = 1357674;
printf("%d\n", c);

int d = -1357674000;                  
printf("%d\n", d);                         

long long e = 3657895000;
printf("%lld\n", e);
/* И тук, както и в предната задаче e = 3657895000 според таблицата 
от лекцията трябва да бъде unsigned int, но при мен не се изписва 
правилно на екрана нито при unsigned int, нито при unsigned long. 
Поради тази причина и тук използвам long long за тази променлива.*/

return 0;
}

